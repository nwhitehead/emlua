{
  "name": "emlua-editor",
  "version": "1.0.0",
  "description": "Editor to play around with Lua code and run it.",
  "main": "src/main.js",
  "dependencies": {
    "moment": "~2.13.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.1",
    "browserify-shim": "^3.8.12"
  },
  "browser": {
    "main": "src/main.js",
    "ace": "./vendor/ace/ace-src-min-noconflict.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-css"
    ]
  },
  "browserify-shim": {
    "ace": "ace"
  },
  "scripts": {
    "build-js": "browserify . > ../build/dist.js",
    "watch-js": "watchify . -o ../build/dist.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nathan Whitehead",
  "license": "ISC"
}
